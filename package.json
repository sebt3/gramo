  "scripts": {
    "lint": "eslint front/ back/ utils/",
    "lint:schema": "graphql-schema-linter $(ls --ignore='gen.*' back/schema/|grep -E 'graphql$'|sed 's#^#back/schema/#')",
    "predev": "webpack build",
    "dev": "ts-node-esm back/index.ts",
    "start": "ts-node-esm back/index.ts",
    "prebuild": "NODE_ENV=production webpack build",
    "build": "tsc -b",
    "postbuild": "mkdir -p dist/back; cp -Rapvf back/schema dist/back/",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
